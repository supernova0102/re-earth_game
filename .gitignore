*.pidb.meta
*.pdb.meta
*.mdb.meta

# Unity3D generated file on crash reports
sysinfo.txt

# Builds
*.apk
*.aab
*.unitypackage
*.app

# Crashlytics generated file
crashlytics-build.properties

# Packed Addressables
re-earth_game/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

